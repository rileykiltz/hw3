<h1><%= @place.name %></h1>

<h2>Entries</h2>
<ul>
  <% @place.entries.each do |entry| %>
    <li>
      <strong><%= entry.title %></strong> - <%= entry.posted_on %>
      <p><%= entry.description %></p>
    </li>
  <% end %>
</ul>

<%= link_to "New Entry", new_place_entry_path(@place) %> |
<%= link_to "Back to Places", places_path %>